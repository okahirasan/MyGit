<form action="/job/create" method="POST" id="sign-up-form" class="form-signin">
	<h2 class="form-signin-heading">Create an account</h2>
	<div class="control-group">
	<input type="text" class="form-control" placeholder="your title" name="title" />
	</div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="contents" name="contents" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="conditions" name="conditions" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="郵便番号" name="zip" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="住所１" name="adress1" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="住所２" name="adress2" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="住所３" name="adress3" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="startDt" name="startDt" />
  </div>

  <div class="control-group">
    <input type="text" class="form-control" placeholder="endDt" name="endDt" />
  </div>

  <div class="control-group">
    <input type="text" class="form-control" placeholder="tags" name="tags" />
  </div>

  <div class="control-group">
    <input type="text" class="form-control" placeholder="company" name="company" />
  </div>

  <div class="control-group">
    <input type="text" class="form-control" placeholder="comment" name="comment" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="jobType" name="jobType" />
  </div>
  <div class="control-group">
    <input type="text" class="form-control" placeholder="jobTypeSalary" name="jobTypeSalary" />
  </div>


  <input type="submit" class="btn btn-lg btn-primary btn-block" value="Create Account"/>
</form>