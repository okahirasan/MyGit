<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
			<script type="text/javascript">
				try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
			</script>
			<ul class="breadcrumb">
				<li>
					<i class="icon-female"></i>
					女の子
				</li>
					<li class="active">新規追加</li>
			</ul>
    </div>
    <div class="page-content">
			<div class="page-header">
					<h1>
						女の子 追加
					</h1>
			</div><!-- /.page-header -->
			<div class="row">
					<div class="col-xs-12">
            <div class="row-fluid">
									<div class="span12">
										<div class="widget-box">
											<div class="widget-header widget-header-blue widget-header-flat">
												<h4 class="lighter">新規　女の子情報</h4>
											</div>
										</div>
									</div>
                  <div class="widget-body">
										<div class="widget-main">
											<div id="fuelux-wizard" class="row-fluid" data-target="#step-container">
												<ul class="wizard-steps">
													<li data-target="#step1" class="active">
														<span class="step">1</span>
														<span class="title">基本情報</span>
													</li>

													<li data-target="#step2">
														<span class="step">2</span>
														<span class="title">詳細情報</span>
													</li>

													<li data-target="#step3">
														<span class="step">3</span>
														<span class="title">各種設定</span>
													</li>

													<li data-target="#step4">
														<span class="step">4</span>
														<span class="title">登録</span>
													</li>
												</ul>
											</div>

											<hr />
                      <form class="form-horizontal" id="validation-form" method="get">
											<div class="step-content row-fluid position-relative" id="step-container">
												<%- partial('./shared/addStep1')%>
												<%- partial('./shared/addStep2')%>
												<%- partial('./shared/addStep3')%>
												<div class="step-pane" id="step4">
													<div class="center">
														<h3 class="green">登録の準備はできました!</h3>
														完了ボタンを押せば登録は完了します!
													</div>
												</div>
											</div>
											<input type="hidden" name="shopid" value="<%= session.Admin.shops[0].id %>" />
											<input type="hidden" name="shopName" value="<%= session.Admin.shops[0].name %>" />
											<input type="hidden" name="shopTypeID" value="<%= session.Admin.shops[0].shopTypeID %>" />
											<input type="hidden" name="_csrf" value="<%= _csrf %>" />
                      </form>
											<hr />
													<div class="row-fluid wizard-actions">
														<button class="btn btn-prev">
															<i class="icon-arrow-left"></i>
															戻る
														</button>

														<button class="btn btn-success btn-next" data-last="完了">
															次へ
															<i class="icon-arrow-right icon-on-right"></i>
														</button>
													</div>
										</div>
									</div>
						</div>
					</div><!-- /.col -->
			</div><!-- /.row -->
    </div>
</div>
